    "cypress-plugin-multiple-click": "^1.0.0",
    "cypress-split": "^1.18.5",
    "cypress-xpath": "^2.0.1",
    "dotenv": "^16.3.1",
    "globals": "^15.0.0",
    "moment": "^2.29.4",
